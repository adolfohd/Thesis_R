erase.commas.at.the.ending <- function(filename){
lines <- readLines(filename)
library(stringr)
for (i in 1:length(lines)){
lines[i] <- str_replace(lines[i], ",$", "")
}
writeLines(lines, filename)
return(NULL)
}
