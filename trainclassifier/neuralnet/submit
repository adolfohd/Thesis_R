universe                = docker
docker_image            = fitohd/thesis_r
executable              = Rscript
arguments               = ann1.r $INT(I) $INT(J)
should_transfer_files   = YES
transfer_input_files    = ann.r,../../lib/,../../data/data.RData,data/,
transfer_output_files   = data
when_to_transfer_output = ON_EXIT
output                  = log/out.$(Process)
error                   = log/err.$(Process)
log                     = log/log.$(Process)
MAX_I = 100 
MAX_J = 10
N = MAX_I * MAX_J
I = ($(Process) / $(MAX_J))+1
J = ($(Process) % $(MAX_J))+1

queue 1
